nrow(aej_maindata)
ncol(aej_maindata)
#select the data from the original dataset
aej_maindata <- as.data.frame(aej_maindata)
selected_data <- select(aej_maindata, dism1990, herf, lenper, povrate_w, povrate_b, area1910, count1910, ethseg10, ethiso10, black1910, passpc, black1920, lfp1920, incseg, pctbk1990, manshr, pop1990)
selected_data <- as.data.frame(selected_data)
#Creating the data for the summary statistics
summary_data <- select(selected_data, dism1990, herf, lenper, povrate_w, povrate_b)
summary_data <- as.data.frame(summary_data)
#Stargazer work - summary stats
stargazer(summary_data, type = "text", title = "Summary Statistics", covariate.labels = c("1990 Dissimilarity Index", "RDI: Railroad Division Index", "Track Length per Square Kilometer", "White Poverty Rate in 1990", "Black Poverty Rate in 1990"))
#Regression - Black OLS
black_pov_reg <- felm(selected_data$povrate_b ~ selected_data$dism1990)
summary(black_pov_reg)
#Regression - White OLS
white_pov_reg <- felm(selected_data$povrate_w ~ selected_data$dism1990)
summary(white_pov_reg)
#Regression - First Stage
instrument_reg <- felm(selected_data$dism1990 ~ selected_data$herf + selected_data$lenper)
summary(instrument_reg)
#Standardizing the variables. We chose these ones because the units don't really mean anything whereas the railroad length variable is very concrete.
selected_data$STdism1990 <- scale(selected_data$dism1990)
selected_data$STherf <- scale(selected_data$herf)
#Regression - Standardized First Stage
STinstrument_reg <- felm(selected_data$STdism1990 ~ selected_data$STherf + selected_data$lenper)
summary(STinstrument_reg)
library(ggplot2)
# Basic scatter plot
ggplot(selected_data, aes(x=herf, y=dism1990)) +
geom_point(size=1.5, shape=23, color = "brown1", fill = "brown1") +
geom_smooth(method="lm", se=TRUE, fullrange=FALSE, level=0.95, color = "brown4") +
labs(y= "1990 Dissimilarity Index", x = "RDI: Railroad Division Index") +
ggtitle("Relationship Between Railroad Division Index and 1990 Dissimilarity Index")
reg1 <- felm(selected_data$black1910 ~ selected_data$herf + selected_data$lenper)
reg2 <- felm(selected_data$incseg ~ selected_data$herf + selected_data$lenper)
reg3 <- felm(selected_data$manshr ~ selected_data$herf + selected_data$lenper)
reg4 <- felm(selected_data$lfp1920 ~ selected_data$herf + selected_data$lenper)
stargazer(reg1, reg2, reg3, reg4, type = "text", dep.var.caption = "Possible Omitted Variables", dep.var.labels = c("% Black - 1910", "Inc. Seg. - 1990", "Share Employed in Manufacturing - 1990", "Labor Force Participation - 1920"), covariate.labels = c("RDI: Railroad Division Index", "Track Length per Square Kilometer"), notes.label = "Significance Levels")
#Black Regression using RDI instrument
black_pov_reg3 <- felm(povrate_b ~ lenper | 0 | (dism1990 ~ herf), selected_data)
#White Regression using RDI Instrument
white_pov_reg3 <- felm(povrate_w ~ lenper | 0 | (dism1990 ~ herf), selected_data)
#Stargazer Work
stargazer(black_pov_reg, black_pov_reg3, white_pov_reg, white_pov_reg3,
type= "text",
title="Effects of Segregation on Black and White Poverty Rates: Instrumental vs. Not Instrumental",
dep.var.caption = "",
dep.var.labels = c("Black Poverty Rate 1990", "White Poverty Rate 1990"),
covariate.labels = c("Diss-I: 1990 Dissimilarity Index", "Diss-I: Instrumented on RDI", "Track Length per Square Kilometer - Control"),
notes.label = "Significance Levels",
add.lines = list(c("Instrumental?", "No", "Yes", "No", "Yes")))
#black graph
ggplot(selected_data, aes(x=herf, y=povrate_b)) +
geom_point(size=1.5, shape=23, color = "cyan3", fill = "cyan3") +
geom_smooth(method="lm", se=TRUE, fullrange=FALSE, level=0.95, color = "cyan4") +
labs(y= "Black Poverty Rate 1990", x = "RDI: Railroad Division Index") +
ggtitle("Relationship Between Railroad Division Index and Black Poverty Rate 1990")
#white graph
ggplot(selected_data, aes(x=herf, y=povrate_w)) +
geom_point(size=1.5, shape=23, color = "darkorchid3", fill = "darkorchid3") +
geom_smooth(method="lm", se=TRUE, fullrange=FALSE, level=0.95, color = "darkorchid4") +
labs(y= "White Poverty Rate 1990", x = "RDI: Railroad Division Index") +
ggtitle("Relationship Between Railroad Division Index and White Poverty Rate 1990")
white_pov_reg3 <- felm(povrate_w ~ lenper | 0 | (dism1990 ~ herf), selected_data)
white_reg2 <- felm(povrate_w ~ lenper + black1910 | 0 | (dism1990 ~ herf), selected_data)
white_reg3 <- felm(povrate_w ~ lenper + incseg | 0 | (dism1990 ~ herf), selected_data)
white_reg4 <- felm(povrate_w ~ lenper + manshr | 0 | (dism1990 ~ herf), selected_data)
white_reg5 <- felm(povrate_w ~ lenper + lfp1920 | 0 | (dism1990 ~ herf), selected_data)
white_reg6 <- felm(povrate_w ~ lenper + black1910 + manshr | 0 | (dism1990 ~ herf), selected_data)
white_reg7 <- felm(povrate_w ~ lenper + incseg + manshr | 0 | (dism1990 ~ herf), selected_data)
black_pov_reg3 <- felm(povrate_b ~ lenper | 0 | (dism1990 ~ herf), selected_data)
black_reg2 <- felm(povrate_b ~ lenper + black1910 | 0 | (dism1990 ~ herf), selected_data)
black_reg3 <- felm(povrate_b ~ lenper + incseg | 0 | (dism1990 ~ herf), selected_data)
black_reg4 <- felm(povrate_b ~ lenper + manshr | 0 | (dism1990 ~ herf), selected_data)
black_reg5 <- felm(povrate_b ~ lenper + lfp1920 | 0 | (dism1990 ~ herf), selected_data)
black_reg6 <- felm(povrate_b ~ lenper + black1910 + manshr | 0 | (dism1990 ~ herf), selected_data)
black_reg7 <- felm(povrate_b ~ lenper + incseg + manshr | 0 | (dism1990 ~ herf), selected_data)
stargazer(black_pov_reg3, black_reg2, black_reg3, black_reg4, black_reg5, black_reg6, black_reg7, type = "text", dep.var.labels = c(""), dep.var.caption = "Black Poverty Rate 1990", title = "Segregation's Impact on Black Poverty Rates in 1990 - Testing Controls", covariate.labels = c("Track Length Per Square Kilometer", "Percent Black 1910", "Income Segregation 1990", "Share Employed in Manuf. 1990", "Labor Force Participation 1920", "Dissimilarty Index 1990 (Instrumented)"), notes.label = "Significance Levels")
stargazer(white_pov_reg3, white_reg2, white_reg3, white_reg4, white_reg5, white_reg6, white_reg7, type = "text", dep.var.labels = c(""), dep.var.caption = "White Poverty Rate 1990", title = "Segregation's Impact on White Poverty Rates in 1990 - Testing Controls", covariate.labels = c("Track Length Per Square Kilometer", "Percent Black 1910", "Income Segregation 1990", "Share Employed in Manuf. 1990", "Labor Force Participation 1920", "Dissimilarty Index 1990 (Instrumented)"), notes.label = "Significance Levels")
#Extracting the coefficient
herf_coef <- summary(instrument_reg)$coef[2,1]
lenper_coef <- summary(instrument_reg)$coef[3,1]
herf_coef
lenper_coef
#Generating predicted values
selected_data$dismestimates <- (selected_data$herf * herf_coef) + (selected_data$lenper * lenper_coef)
#Black Regression using estimates
black_est_reg <- felm(selected_data$povrate_b ~ selected_data$dismestimates)
summary(black_est_reg)
#White Regression using estimates
white_est_reg <- felm(selected_data$povrate_w ~ selected_data$dismestimates)
summary(white_est_reg)
#Putting this into Stargazer for fun/convenience.
stargazer(black_pov_reg, black_est_reg, white_pov_reg, white_est_reg,
